# VERSATIL SDLC Framework - Production Environment Configuration
# Supabase + Edge Functions for Agentic RAG

# Supabase Configuration
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# OpenAI Configuration for Embeddings
OPENAI_API_KEY=your-openai-api-key-here

# Production Settings
NODE_ENV=production
ENHANCED_AGENTS_ENABLED=true
PERFORMANCE_MONITORING=true
RAG_EDGE_FUNCTIONS=true

# Agent Configuration
MARIA_RAG_ENABLED=true
JAMES_RAG_ENABLED=true
MARCUS_RAG_ENABLED=true

# Vector Database Settings
VECTOR_DIMENSION=1536
SIMILARITY_THRESHOLD=0.7
MAX_RAG_EXAMPLES=5

# Edge Function URLs (auto-generated after deployment)
MARIA_RAG_URL=https://your-project-ref.supabase.co/functions/v1/maria-rag
JAMES_RAG_URL=https://your-project-ref.supabase.co/functions/v1/james-rag
MARCUS_RAG_URL=https://your-project-ref.supabase.co/functions/v1/marcus-rag

# Performance Monitoring
AGENT_PERFORMANCE_TRACKING=true
RAG_RESPONSE_TIME_THRESHOLD=2000
EDGE_FUNCTION_TIMEOUT=30000

# Security Settings
CORS_ORIGINS=https://your-domain.com,https://localhost:3000
JWT_VERIFY=false
API_RATE_LIMIT=100

# Logging Configuration
LOG_LEVEL=info
RAG_LOGGING=true
AGENT_TELEMETRY=true