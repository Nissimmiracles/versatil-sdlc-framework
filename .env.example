# Playwright MCP Configuration (Official Microsoft @playwright/mcp)
PLAYWRIGHT_BROWSERS_PATH=~/.cache/ms-playwright
PLAYWRIGHT_MCP_HEADLESS=false
PLAYWRIGHT_MCP_TIMEOUT=30000

