{
  "name": "production",
  "description": "High-availability production environment with enterprise-grade monitoring and security",
  "version": "1.2.0",
  "settings": {
    "debug": false,
    "hotReload": false,
    "mockExternalServices": false,
    "strictValidation": true,
    "verboseLogging": false,
    "optimizations": true
  },
  "services": {
    "mcp": {
      "host": "0.0.0.0",
      "port": 3000,
      "ssl": true,
      "autoRestart": true,
      "watchFiles": false,
      "healthCheck": true,
      "clustering": true,
      "workers": 4
    },
    "database": {
      "type": "postgresql",
      "url": "${DATABASE_URL}",
      "pool": {
        "min": 5,
        "max": 20
      },
      "logging": false,
      "ssl": true,
      "readReplicas": true,
      "connectionTimeout": 30000
    },
    "supabase": {
      "url": "${SUPABASE_PRODUCTION_URL}",
      "anonKey": "${SUPABASE_PRODUCTION_ANON_KEY}",
      "serviceKey": "${SUPABASE_PRODUCTION_SERVICE_KEY}",
      "local": false,
      "edgeFunctions": true,
      "globalDistribution": true
    },
    "opera": {
      "enabled": true,
      "autoUpdate": false,
      "updateChannel": "stable",
      "maxConcurrentGoals": 10,
      "timeout": 600000,
      "clustering": true,
      "distributedMode": true
    }
  },
  "agents": {
    "maria-qa": {
      "enabled": true,
      "autoActivate": true,
      "debugMode": false,
      "testCoverageThreshold": 90,
      "performanceThreshold": 98,
      "strictMode": true,
      "productionValidation": true,
      "continuousMonitoring": true
    },
    "james-frontend": {
      "enabled": true,
      "autoActivate": true,
      "debugMode": false,
      "optimizations": true,
      "bundleOptimization": true,
      "performanceMonitoring": true,
      "realUserMonitoring": true
    },
    "marcus-backend": {
      "enabled": true,
      "autoActivate": true,
      "debugMode": false,
      "productionOptimizations": true,
      "securityHardening": true,
      "performanceMonitoring": true,
      "distributedTracing": true
    },
    "sarah-pm": {
      "enabled": true,
      "autoActivate": true,
      "reportingInterval": 900000,
      "businessMetrics": true,
      "stakeholderReporting": true,
      "executiveDashboard": true
    },
    "alex-ba": {
      "enabled": true,
      "autoActivate": true,
      "businessValidation": true,
      "userBehaviorAnalysis": true,
      "conversionTracking": true
    },
    "dr-ai-ml": {
      "enabled": true,
      "autoActivate": true,
      "productionModels": true,
      "gpu": true,
      "modelValidation": true,
      "a-bTesting": true,
      "scaledInference": true
    },
    "devops-dan": {
      "enabled": true,
      "autoActivate": true,
      "infrastructureMonitoring": true,
      "deploymentAutomation": true,
      "rollbackCapability": true,
      "autoScaling": true,
      "disasterRecovery": true
    },
    "security-sam": {
      "enabled": true,
      "autoActivate": true,
      "vulnerabilityScanning": true,
      "complianceValidation": true,
      "securityMonitoring": true,
      "threatDetection": true,
      "incidentResponse": true
    }
  },
  "testing": {
    "runTestsOnChange": false,
    "parallelTesting": false,
    "productionTesting": false,
    "smokeTesting": true,
    "coverage": {
      "threshold": 90,
      "failOnLow": true
    },
    "canaryTesting": true,
    "a-bTesting": true
  },
  "monitoring": {
    "enabled": true,
    "apm": true,
    "distributedTracing": true,
    "metricsCollection": true,
    "performanceTracking": true,
    "errorTracking": true,
    "logLevel": "error",
    "alerting": true,
    "businessMetrics": true,
    "realUserMonitoring": true,
    "syntheticMonitoring": true,
    "uptime": true
  },
  "security": {
    "authRequired": true,
    "encryptionEnabled": true,
    "auditLogging": true,
    "vulnerabilityScanning": true,
    "complianceFramework": "SOC2",
    "securityHeaders": true,
    "rateLimiting": true,
    "ddosProtection": true,
    "waf": true,
    "secretsManagement": true
  },
  "features": {
    "ragMemory": true,
    "operaOrchestrator": true,
    "realTimeUpdates": true,
    "autoHealing": true,
    "distributedMode": true,
    "canaryDeployment": true,
    "featureFlags": true,
    "a-bTesting": true,
    "personalizedExperience": true
  },
  "deployment": {
    "strategy": "blue-green",
    "rollbackEnabled": true,
    "healthChecks": true,
    "gracefulShutdown": true,
    "loadBalancing": true,
    "autoScaling": true,
    "multiRegion": true,
    "cdn": true
  },
  "performance": {
    "caching": {
      "enabled": true,
      "strategy": "multi-layer",
      "redis": true,
      "cdn": true,
      "edgeCache": true
    },
    "optimization": {
      "bundleOptimization": true,
      "lazyLoading": true,
      "imageOptimization": true,
      "compressionEnabled": true
    },
    "scaling": {
      "autoScaling": true,
      "minInstances": 2,
      "maxInstances": 20,
      "targetCpuUtilization": 70,
      "scaleUpCooldown": 300,
      "scaleDownCooldown": 600
    }
  },
  "compliance": {
    "framework": "SOC2",
    "auditTrail": true,
    "dataRetention": "7y",
    "accessControl": true,
    "dataProtection": true,
    "gdprCompliant": true,
    "hipaaCompliant": false,
    "iso27001": true
  },
  "backup": {
    "enabled": true,
    "frequency": "6h",
    "retention": "30d",
    "crossRegion": true,
    "pointInTimeRecovery": true,
    "encryptedBackups": true
  },
  "disaster_recovery": {
    "enabled": true,
    "rto": "4h",
    "rpo": "1h",
    "multiRegion": true,
    "failoverTesting": true,
    "automaticFailover": false
  }
}